<Window x:Class="UI_WPF.VolunteerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI_WPF"
        mc:Ignorable="d"
        Title="VolunteerWindow" Height="450" Width="800"
        Loaded="Window_Loaded">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" >
        <Button x:Name="btnViewCalls" Content="View all Calls" Width="200" Click="BtnViewCalls_Click"/>
        <Button x:Name="btnAddCall" Content="Add Call" Width="200" Click="BtnAddCall_Click"/>
    </StackPanel>
</Window>
